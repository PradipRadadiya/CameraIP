<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Trailer.fragment.activity.TrailerAct">

    <com.packetalk.utility.SpeedometerView
            android:id="@+id/meterView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"

            app:speed="90" />



    <com.packetalk.utility.MyTextView
            android:id="@+id/myTextView29"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="26.68 V"
            android:textColor="@color/white"
            android:textSize="@dimen/twenty_four_dp"
            app:layout_constraintBottom_toBottomOf="@+id/meterView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/meterView" />

</androidx.constraintlayout.widget.ConstraintLayout>